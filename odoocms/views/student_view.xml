    <record id="view_odoocms_student_comments_search" model="ir.ui.view">
        <field name="name">odoocms.student.comments.search</field>
        <field name="model">odoocms.student.comment</field>
        <field name="arch" type="xml">
            <search string="Student Comments">
                <field name="name" string="Student"
                       filter_domain="['|', ('name','ilike',self), ('code','ilike',self)]"/>
                <field name="message_ref" string="Reference" filter_domain="[('message_ref','ilike',self)]"/>
                <newline/>
                <group expand="0" string="Group By...">
                    <filter name="student" string='Student' domain="[]" context="{'group_by':'student_id'}"/>

                    <filter name="program" string="Program" domain="[]" context="{'group_by':'program_id'}"/>
                    <filter name="date" string='Date' domain="[]" context="{'group_by':'date'}"/>
                </group>

            </search>
        </field>
    </record>

    <record id="action_odoocms_student_comments" model="ir.actions.act_window">
        <field name="name">Student Comments</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">odoocms.student.comment</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">Click to register new Student Comment.</p>
        </field>
    </record>


    <record id="view_odoocms_student_profilechange_attribute_form" model="ir.ui.view">
        <field name="name">odoocms.student.profilechange.attribute.form</field>
        <field name="model">odoocms.student.profilechange.attribute</field>
        <field name="arch" type="xml">
            <form string="Add Profile Change Attributes">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="field_name_id"
                                   domain="[('model','=','odoocms.student'),('ttype','in',('char', 'integer','float', 'date')),('related','=',False)]"
                                   options="{'no_create': True, 'no_quick_create':True, 'no_create_edit': True, 'limit': 10}"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_odoocms_student_profilechange_attribute_tree" model="ir.ui.view">
        <field name="name">odoocms.student.profilechange.attribute.tree</field>
        <field name="model">odoocms.student.profilechange.attribute</field>
        <field name="arch" type="xml">
            <tree string="Attributes allowed for Student Profile Change">
                <field name="name"/>
                <field name="field_name_id"/>
            </tree>
        </field>
    </record>

    <record id="action_odoocms_student_profilechange_attribute" model="ir.actions.act_window">
        <field name="name">Students Profile Change Attributes</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">odoocms.student.profilechange.attribute</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">Click to register new Attributes.</p>
        </field>
    </record>

    <record id="action_odoocms_student_process_pictures" model="ir.actions.server">
        <field name="name">Process Pictures</field>
        <field name="type">ir.actions.server</field>
        <field name="model_id" ref="model_odoocms_student"/>
        <field name="state">code</field>
        <field name="code">action = model.process_images()</field>
    </record>

    <record id="view_odoocms_student_tag_category_tree" model="ir.ui.view">
        <field name="name">odoocms.student.tag.category.tree</field>
        <field name="model">odoocms.student.tag.category</field>
        <field name="arch" type="xml">
            <tree string="Tag Category">
                <field name="name"/>
                <field name="code"/>
                <field name="multiple"/>
            </tree>
        </field>
    </record>
    <record id="view_odoocms_student_tag_category_form" model="ir.ui.view">
