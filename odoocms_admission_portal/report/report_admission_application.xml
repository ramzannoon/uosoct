<odoo>
    <template id="odoocms_admission_portal.report_admission">
        <t t-call="web.html_container">
            <t t-foreach="application_ids" t-as="o">
                <t t-call="odoocms_admission_portal.report_admission_final_document"/>
            </t>
        </t>
    </template>
