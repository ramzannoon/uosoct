    <template id="odoocms_admission_portal.report_admission_final_document">
        <t t-call="web.html_container">
            <t t-if="not company">
                <t t-if="company_id">
                    <t t-set="company" t-value="company_id"/>
                </t>
                <t t-elif="o and 'company_id' in o">
                    <t t-set="company" t-value="o.company_id.sudo()"/>
                </t>
                <t t-else="else">
                    <t t-set="company" t-value="res_company"/>
                </t>
            </t>
            <t t-call="web.basic_layout">
                <style>
                    .borders{
                    border: 1px solid black;
                    }
                    .margins{
                    margin-top: 3%;
                    }
                    .signature {
                    border: 0;
                    border-bottom: 1px solid #000;
                    }

                </style>
                <div class="page">
                    <div class="row" style="padding:2%;">
                        <div class="col col-12">
                            <t t-if="not o ">
                                <span>Sorry no record found</span>
                            </t>
                            <t t-foreach="o" t-as="o">
                                <div class="row">
                                    <div class="col col-2" style="text-align:left;">
                                        <img style="width:120px;height:120px;"
                                             t-att-src="image_data_uri(company_id.logo)" t-if="company_id.logo"/>
                                        <img src="/web/static/src/img/user_menu_avatar.png"
                                             t-if="not company_id.logo"/>
                                    </div>
                                    <div class="col col-10" style="text-align:center;" id="admission_form_report_head">
                                        <h1 style="color:red;" t-esc="company.name" t-if="company.name"/>
                                        <h6 style="text-align: center;">Main I.J.P Road, Sector I-12, Islamabad</h6>
                                        <h5 style="text-align: center; color:red;border-bottom: 1px solid black double;">
                                            CANDIDATE REGEISTERATION FOR UG ADMISSION <span t-esc="register.term_id.name" />
                                        </h5>
                                        <!--<h1 t-if="not company_id.name">Admit Card</h1>-->
                                    </div>
                                </div>
                                <div class="row margins">
                                    <div class="col col-10" style="text-align:left;">
                                        <div class="row" style="margin-bottom:2%;">
                                            <div class="col col-2 " style="text-align:left;">
                                                Name:
                                            </div>
                                            <div class="col col-4 "
                                                 style="text-align:left; border-bottom:1px solid black;">
                                                <t t-esc="o.name"/>
                                            </div>
                                            <div class="col col-2 " style="text-align:left;">
                                                CNIC:
                                            </div>
                                            <div class="col col-4 "
                                                 style="text-align:left; border-bottom:1px solid black;">
                                                <t t-esc="o.cnic"/>
                                            </div>

                                        </div>
                                        <div class="row" style="margin-bottom:2%;">
                                            <div class="col col-2 " style="text-align:left;">
                                                Email:
                                            </div>
                                            <div class="col col-4"
                                                 style="text-align:left; border-bottom:1px solid black;">
                                                <t t-esc="o.email"/>
                                            </div>
                                            <div class="col col-2 " style="text-align:left;">
                                                Contact:
                                            </div>
                                            <div class="col col-4"
                                                 style="text-align:left;border-bottom:1px solid black;">
                                                <t t-esc="o.mobile"/>
                                            </div>
                                        </div>
                                        <div class="row" style="margin-bottom:2%;">
                                            <div class="col col-2" style="text-align:left;">
                                                DOB:
                                            </div>
                                            <div class="col col-2"
                                                 style="text-align:left; border-bottom:1px solid black;">

                                                <t t-esc="o.date_of_birth.strftime('%d-%m-%Y')"/>
                                            </div>
                                            <div class="col col-2" style="text-align:left;">
                                                Gender:
                                            </div>
                                            <div class="col col-2 "
                                                 style="text-align:left;border-bottom:1px solid black;">
                                                <t t-if="o.gender == 'm'">
                                                    Male
                                                </t>
                                                <t t-elif="o.gender == 'f'">
                                                    Female
                                                </t>
                                                <t t-elif="o.gender == 'o'">
                                                    Other
                                                </t>
                                            </div>
                                            <div class="col col-2 " style="text-align:left;">
                                                Nationality
                                            </div>
                                            <div class="col col-2"
                                                 style="text-align:left;border-bottom:1px solid black;">
                                                <t t-esc="o.nationality.name"/>
                                            </div>
                                        </div>
                                        <div class="row" style="margin-bottom:2%;">
                                            <div class="col col-3" style="text-align:left;">
                                                Religion &amp; Sect:
                                            </div>
                                            <div class="col col-9"
                                                 style="text-align:left; border-bottom:1px solid black;">
                                                <t t-esc="o.religion_id.name"/>
                                            </div>
                                        </div>

                                    </div>
                                    <!--<div class="col col-1"/>-->
                                    <div class="col col-2" style="text-align:center;">
                                        <t t-if="application_ids.image">
                                            <img id="profile_image borders"
                                                 t-att-src="'data:image/png;base64,%s' % to_text(application_ids.image)"
                                                 width="200px" height="200px"/>
                                        </t>
                                        <t t-if="not application_ids.image ">Passport Size Photograph</t>
                                    </div>
                                </div>
                            </t>

                            <div class="row">
                                <div class="col col-12 margins">
                                    <div class="row" style="margin-bottom:2%;">
                                        <div class="col col-2" style="text-align:left;">
                                            Province:
                                        </div>
                                        <div class="col col-2"
                                             style="text-align:left; border-bottom:1px solid black;">
                                            <t t-esc="o.province_id.name"/>
                                        </div>
                                        <div class="col col-2" style="text-align:left;">
                                            Domicile:
                                        </div>
                                        <div class="col col-2 "
                                             style="text-align:left;border-bottom:1px solid black;">
                                            <t t-esc="o.domicile_id.name"/>
                                        </div>
                                        <div class="col col-2 " style="text-align:left;">
                                            City:
                                        </div>
                                        <div class="col col-2"
                                             style="text-align:left;border-bottom:1px solid black;">
                                            <t t-esc="o.per_city"/>
                                        </div>
                                    </div>

                                    <div class="row" style="margin-bottom:2%;">

                                        <div class="col col-3" style="text-align:left;">
                                            Program Choices:
                                        </div>
                                        <div class="col col-9"
                                             style="text-align:left; border-bottom:1px solid black;">
                                            <t t-set="priority" t-value="0"/>
                                            <t t-foreach="program_preferences_ordered" t-as="program">
                                                <t t-set="priority" t-value="priority + 1"/>
                                                [<b t-esc="priority"/>]
                                                <!-- <th class="text-left">Preference </th> -->
                                                <!--<td class="text-left">-->
                                                <span t-field="program.program_id.code"/> &#x9;
                                                <!--</td>-->
                                            </t>
                                        </div>
                                    </div>
                                    <div class="row" style="margin-bottom:2%;">
                                        <div class="col col-2" style="text-align:left;">
                                            Present Address:
                                        </div>
                                        <div class="col col-10"
                                             style="text-align:left; border-bottom:1px solid black;">
                                            <t t-esc="o.street"/>, <t t-esc="o.street2"/>, <t t-esc="o.city"/>, <t t-esc="o.present_country_id.name"/>
                                        </div>
                                    </div>
                                    <div class="row" style="margin-bottom:2%;">
                                        <div class="col col-3" style="text-align:left;">
                                            Permanent Address:
                                        </div>
                                        <div class="col col-9"
                                             style="text-align:left; border-bottom:1px solid black;">
                                            <t t-esc="o.per_street"/>, <t t-esc="o.per_street2"/>, <t t-esc="o.per_city"/>, <t t-esc="o.per_country_id.name"/>
                                        </div>
                                    </div>
                                    <div class="row" style="margin-bottom:2%;">
                                        <div class="col col-3" style="text-align:left;">
                                            Disability?:
                                        </div>
                                        <div class="col col-9">
                                            <t t-if="o.is_any_disability== True " >
                                            Yes &#x9; Details: <span style="text-align:left; border-bottom:1px solid black;"><t t-esc="o.disability"/></span>
                                        </t>
                                             <t t-if="not o.is_any_disability==True" >
                                            No
                                        </t>
                                        </div>
                                    </div>
                                    <div class="row" style="margin-bottom:2%;">
                                        <div class="col col-5" style="text-align:left;">
                                            <h4 style="">
                                                <span style="border-bottom:1px solid black;">Father / Guardian</span>
                                            </h4>
                                        </div>
                                    </div>
                                    <div class="row" style="margin-bottom:2%;">
                                        <div class="col col-2" style="text-align:left;">
                                            Name:
                                        </div>
                                        <div class="col col-4"
                                             style="text-align:left; border-bottom:1px solid black;">
                                            <t t-esc="o.father_name"/>
                                           <t t-if="o.guardian_name"><t t-esc="o.guardian_name"/></t>
                                        </div>
                                        <div class="col col-2" style="text-align:left;">
                                            Relation:
                                        </div>
                                        <div class="col col-4"
                                             style="text-align:left; border-bottom:1px solid black;">
                                          <t t-if="o.guardian_relation"> <t t-esc="o.guardian_relation"/></t>
                                            <t t-if="not o.guardian_relation"> Father</t>
                                        </div>
                                    </div>
                                    <div class="row" style="margin-bottom:2%;">
                                        <div class="col col-2" style="text-align:left;">
                                            Contact No:
                                        </div>
                                        <div class="col col-10"
                                             style="text-align:left; border-bottom:1px solid black;">
                                            <t t-esc="o.guardian_mobile"/>
                                        </div>
                                    </div>
                                    <div class="row" style="margin-bottom:2%;">
                                        <div class="col col-5" style="text-align:left;">
                                            <h4 style="">
                                                <span style="border-bottom:1px solid black;">Academic Details</span>
