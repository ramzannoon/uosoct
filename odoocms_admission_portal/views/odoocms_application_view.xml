    <!-- Application Preferences Tree -->
    <record id="view_odoocms_application_prefer_tree" model="ir.ui.view">
        <field name="name">odoocms.application.prefer.tree</field>
        <field name="model">odoocms.application.preference</field>
        <field name="priority" eval="8"/>
        <field name="arch" type="xml">
            <tree string="Application Preference">
                <field name="application_id"/>
                <!--                <field name="application_status"/>-->
                <field name="cnic" string="CNIC"/>
                <field name="program_id"/>
                <field name="preference"/>
            </tree>
        </field>
    </record>

    <!-- Application Preferences Form -->
    <record id="view_odoocms_application_prefer_form" model="ir.ui.view">
        <field name="name">odoocms.application.prefer.form</field>
        <field name="model">odoocms.application.preference</field>
        <field name="priority" eval="8"/>
        <field name="arch" type="xml">
            <form string="Application Preference">
                <sheet>
                    <group>
                        <group>
                            <field name="application_id"/>
                            <field name="cnic" string="CNIC"/>
                        </group>
                        <group>
                            <field name="program_id"/>
                            <field name="preference"/>
                        </group>
                    </group>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="activity_ids" widget="mail_activity"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
            </form>
        </field>
    </record>

    <!-- Application Preferences Search -->
    <record id="view_odoocms_application_prefer_search" model="ir.ui.view">
        <field name="name">odoocms.application.prefer.search</field>
        <field name="model">odoocms.application.preference</field>
        <field name="priority" eval="8"/>
        <field name="arch" type="xml">
            <search string="Application Preference">
                <field name="application_id"/>
                <field name="cnic" string="CNIC"/>
                <field name="program_id"/>
                <field name="preference"/>

                <newline/>
                <group expand="0" string="Group By...">
                    <!--                    <filter string="Application Status" domain="[]" name="groupby_application" context="{'group_by':'application_status'}"/>-->
                    <filter string="Program" domain="[]" name="groupby_program" context="{'group_by':'program_id'}"/>
                    <filter string="Preference" domain="[]" name="groupby_preference"
                            context="{'group_by':'preference'}"/>
                    <separator/>
                </group>
            </search>
        </field>
    </record>

