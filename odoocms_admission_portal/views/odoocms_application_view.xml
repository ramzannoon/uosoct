    <record id="view_odoocms_application_tree_inherit" model="ir.ui.view">
        <field name="name">odoocms.application.tree.inherit</field>
        <field name="model">odoocms.application</field>
        <field name="inherit_id" ref="odoocms_admission.view_odoocms_application_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='state']" position="before">
                <field name="fee_voucher_state"/>
                <field name="total_aggregates"/>
                <field name="step_number" optional="hide"/>
            </xpath>
        </field>
    </record>

    <record id="view_odoocms_application_search_inherit" model="ir.ui.view">
        <field name="name">odoocms.application.search.inherit</field>
        <field name="model">odoocms.application</field>
        <field name="inherit_id" ref="odoocms_admission.view_odoocms_application_search"/>
        <field name="arch" type="xml">
            <xpath expr="//filter[@name='religion_id']" position="after">
                <separator/>
                <filter name="group_admission_quota" string="Admission Quota" icon="fa-pencil-square-o" domain="[]"
                        context="{'group_by' : 'admission_quota_id'}"/>
            </xpath>
            <xpath expr="//filter[@name='rejected']" position="after">
                <filter string="Applied on Open Merit Only" name="applied_on_open_merit"
                        domain="[('quota_id','=',False),('quota_id2','=',False)]"/>
                <filter string="Applied on Forces Quota" name="applied_on_forces_quota"
                        domain="[('quota_id','=',False),('quota_id2','!=',False)]"/>
                <filter string="Applied on Rural Areas Quota" name="applied_on_rural_quota"
                        domain="[('quota_id','!=',False),('quota_id2','=',False)]"/>
                <filter string="Applied on Rural and Forces Quota" name="applied_on_rural_forces_quota"
                        domain="[('quota_id','!=',False),('quota_id2','!=',False)]"/>
                <filter string="Admission Offered" name="admission_offered" domain="[('program_id','!=',False)]"/>
                <filter string="Admission not Offered" name="admission_offered" domain="[('program_id','=',False)]"/>
                <filter string="Admission on Quota" name="admission_on_quota"
                        domain="[('program_id','!=',False),('admission_quota_id','!=',False)]"/>
                <filter string="Admission on Open Merit" name="admission_on_open_merit"
                        domain="[('program_id','!=',False),('admission_quota_id','=',False)]"/>
            </xpath>
        </field>
    </record>


