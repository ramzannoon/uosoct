    <!-- Academic Documents -->
    <record id="view_odoocms_application_documents_form_inherit" model="ir.ui.view">
        <field name="name">odoocms.application.documents.form.inherit</field>
        <field name="model">odoocms.application.documents</field>
        <field name="inherit_id" ref="odoocms_admission.view_odoocms_application_documents_form"/>
        <field name="arch" type="xml">
            <data>
                <xpath expr="//table/tr[@name='third_row']" position="after">
                    <tr name="fourth_row">
                        <td name="dae_first_year_heading">DAE First Year</td>
                        <td class="w-25" name="dae_first_year_image">
                            <field name="dae_first_year" widget='image' nolabel="1" class="rounded float-left"/>
                        </td>
                        <td name="cnic_heading">CNIC Back</td>
                        <td class="w-25" name="cnic_back_image">
                            <field name="cnic_back_scanned_copy" widget='image' nolabel="1" class="rounded float-left"/>
                        </td>
                    </tr>

                    <tr name="fifth_row">
                        <td name="dae_second_year_heading">DAE Second Year</td>
                        <td class="w-25" name="dae_second_year_image">
                            <field name="dae_second_year" widget='image' nolabel="1" class="rounded float-left"/>
                        </td>
                        <td></td>
                        <td></td>
                    </tr>

                    <tr name="sixth_row">
                        <td name="dae_third_year_heading">DAE Third Year</td>
                        <td class="w-25" name="dae_third_year_image">
                            <field name="dae_third_year" widget='image' nolabel="1" class="rounded float-left"/>
                        </td>
                        <td></td>
                        <td></td>
                    </tr>

                    <tr name="seven_row">
                        <td name="test_heading">Test</td>
                        <td class="w-25" name="test_image">
                            <field name="test_certificate" widget='image' nolabel="1" class="rounded float-left"/>
                        </td>
                        <td></td>
                        <td></td>
                    </tr>
                </xpath>
            </data>
        </field>
    </record>


    <!-- Academic Documents Tree-->
    <record id="view_odoocms_application_documents_tree_inherit" model="ir.ui.view">
        <field name="name">odoocms.application.documents.tree.inherit</field>
