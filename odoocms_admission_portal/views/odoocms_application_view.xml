<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_odoocms_application_steps_tree" model="ir.ui.view">
        <field name="name">odoocms.application.steps.tree</field>
        <field name="model">odoocms.application.steps</field>
        <field name="arch" type="xml">
            <tree string="Application Steps" editable="bottom">
                <field name="sequence"/>
                <field name="name"/>
                <field name="template"
                       options="{'no_create': True, 'no_quick_create':True, 'no_create_edit': True, 'limit': 10}"/>
                <field name="test_field"
                       options="{'no_create': True, 'no_quick_create':True, 'no_create_edit': True, 'limit': 10}"/>
                <field name="active"/>
            </tree>
        </field>
    </record>

    <record id="view_odoocms_application_form_inherit" model="ir.ui.view">
        <field name="name">odoocms.application.form.inherit</field>
        <field name="model">odoocms.application</field>
        <field name="inherit_id" ref="odoocms_admission.view_odoocms_application_form"/>
        <field name="arch" type="xml">
            <data>
                <xpath expr="//button[@name='send_to_verify']" position="attributes">
                    <attribute name="invisible">0</attribute>
                </xpath>

                <xpath expr="//notebook/page/field/tree/field[@name='total_marks']" position="attributes">
                    <attribute name="invisible">0</attribute>
                </xpath>

                <xpath expr="//notebook/page/field/tree/field[@name='obtained_marks']" position="attributes">
                    <attribute name="invisible">0</attribute>
                </xpath>

                <xpath expr="//notebook/page/field/tree/field[@name='percentage']" position="attributes">
                    <attribute name="invisible">0</attribute>
                </xpath>

                <xpath expr="//notebook/page/field/tree/field[@name='board']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>

                <xpath expr="//header" position="inside">
                    <button name="verify_voucher" type="object" class="oe_highlight"
                            string="Verify Fee Voucher" groups="base.group_user"
                            help="Click to verify the Voucher if it is valid"
                            attrs="{'invisible': [('fee_voucher_state', '!=','upload')]}"
                    />
                    <button name="univerify_voucher" type="object" class="oe_highlight"
                            string="Unverify Fee Voucher" groups="base.group_user"
                            help="Click to Unverify the Voucher if it is not valid"
                            attrs="{'invisible': [('fee_voucher_state', '!=','upload')]}"
                    />
                </xpath>
                <xpath expr="//field[@name='cnic']" position="after">
                    <field name="passport"/>
                </xpath>

                <xpath expr="//field[@name='domicile']" position="replace">
                    <field name="domicile_id"
                           attrs="{'invisible':[('nationality','!=',177)]}"/> <!-- 177 === PAKISTAN -->
                </xpath>
                <xpath expr="//field[@name='domicile_id']" position="after">
                    <field name="get_info_from"/>
                </xpath>

                <xpath expr="//field[@name='nationality']" position="after">
                    <field name="province_id"/>
                    <field name="province2" attrs="{'invisible':[('province_id','!=',False)]}"/>
                </xpath>

                <xpath expr="//field[@name='country_id']" position="replace">
                    <field name="present_country_id" placeholder="Country" class="oe_no_button"
                           options='{"no_open": True}'/>
                </xpath>

                <xpath expr="//group/field[@name='board']" position="after">
                    <group>
                        <field name="inter_result_status" attrs="{'invisible':[('degree_level','!=','Intermediate')]}"/>
                    </group>
                </xpath>

                <xpath expr="//group[@name='dae_group']" position="after">
                    <!-- make visible if O Level -->
                    <group string="O-Level" name="o_level_group"
                           attrs="{'invisible':[('degree_level','!=','O-Level')]}">
                        <table class="table table-bordered">
                            <tr bgcolor="#a8aba3">
                                <th>Grade</th>
                                <th>Score</th>
                            </tr>
                            <tr>
                                <th>A*</th>
                                <td>
                                    <field name="grade_aa" nolabel="1"/>
                                </td>
                            </tr>
                            <tr>
                                <th>A</th>
                                <td>
                                    <field name="grade_a" nolabel="1"/>
                                </td>
                            </tr>
                            <tr>
                                <th>B</th>
                                <td>
                                    <field name="grade_b" nolabel="1"/>
                                </td>
                            </tr>
                            <tr>
                                <th>C</th>
                                <td>
                                    <field name="grade_c" nolabel="1"/>
                                </td>
                            </tr>
                            <tr>
