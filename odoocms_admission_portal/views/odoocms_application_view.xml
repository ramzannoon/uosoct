                            </tr>
                            <tr>
                                <th>G</th>
                                <td>
                                    <field name="grade_g" nolabel="1"/>
                                </td>
                            </tr>
                        </table>
                    </group>
                    <group name="o_level_group2" attrs="{'invisible':[('degree_level','!=','O-Level')]}">
                        <table class="table table-bordered">
                            <tr bgcolor="#a8aba3">
