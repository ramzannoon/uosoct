<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_odoocms_alternative" model="ir.sequence">
            <field name="name">Alternative Course</field>
            <field name="code">odoocms.student.course.alternate</field>
            <field name="prefix">ALT</field>
            <field name="padding">4</field>
        </record>

        <record id="seq_odoocms_student_rechecking_tag" model="ir.sequence">
            <field name="name">Rechecking Tag</field>
            <field name="code">odoocms.request.subject.rechecking</field>
            <field name="prefix">RE</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_odoocms_disposal_history_tag" model="ir.sequence">
            <field name="name">Disposal History Tag</field>
            <field name="code">odoocms.exam.disposal.history</field>
            <field name="prefix">DIS</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_odoocms_student_course_incomplete" model="ir.sequence">
            <field name="name">Student Course Incomplete</field>
            <field name="code">odoocms.student.course.incomplete</field>
            <field name="prefix">I-</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_odoocms_student_course_retest" model="ir.sequence">
            <field name="name">Student Course Retest</field>
            <field name="code">odoocms.student.course.retest</field>
            <field name="prefix">RET</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_odoocms_student_grade_change" model="ir.sequence">
            <field name="name">Student Grade Change</field>
            <field name="code">odoocms.student.grade.change</field>
            <field name="prefix">SGC-</field>
            <field name="padding">4</field>
        </record>

        <record id="seq_odoocms_course_grade_change" model="ir.sequence">
            <field name="name">Course Grade Change</field>
            <field name="code">odoocms.course.grade.change</field>
            <field name="prefix">CGC-</field>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>