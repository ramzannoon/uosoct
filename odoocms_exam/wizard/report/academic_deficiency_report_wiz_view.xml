<?xml version="1.0"?>
<odoo>

    <record id="academic_deficiency_warning_wiz_view_form" model="ir.ui.view">
        <field name="name">academic.deficiency.warning.wiz.form</field>
        <field name="model">academic.deficiency.warning.wiz</field>
        <field name="arch" type="xml">
            <form string="Academic Deficiency(Warning)">
                <group>
                	<group>
                        <field name="batch_id" options="{'no_quick_create':True, 'no_create_edit': True, 'limit': 10}"/>
                    </group>
                </group>
                <footer>
                    <button name="print_report" string="Generate Report" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-default" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

    <record id="action_academic_deficiency_warning_wiz" model="ir.actions.act_window">
        <field name="name">Academic Deficiency(Warning)</field>
        <field name="res_model">academic.deficiency.warning.wiz</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

     <menuitem
            id="menu_academic_deficiency_warning_wiz"
            name="Academic Deficiency(Warning)"
            action="action_academic_deficiency_warning_wiz"
            parent="odoocms_exam.menu_odoocms_exam_report"
            sequence="200"/>


