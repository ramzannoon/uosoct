<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for op.admission -->
       <record id="seq_odoocms_application_fall" model="ir.sequence">
            <field name="name">Student Application</field>
            <field name="code">odoocms.application.fall</field>
            <field name="prefix">F-%(range_year)s-</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_odoocms_application_spring" model="ir.sequence">
            <field name="name">Student Application</field>
            <field name="code">odoocms.application.spring</field>
            <field name="prefix">S-%(range_year)s-</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_odoocms_application_summer" model="ir.sequence">
            <field name="name">Student Application</field>
            <field name="code">odoocms.application.summer</field>
            <field name="prefix">Sum-%(range_year)s-</field>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>
